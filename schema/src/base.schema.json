{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type":"object",
  "definitions": {
    "bool": {
      "type": "boolean"
    },
    "date": {
      "type": "integer"
    },
    "duration": {
      "type": "integer",
      "description": "Length of duration in seconds"
    },
    "uuid": {
      "type":"string",
      "pattern":"[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}"
    },
    "uuidArray": {
      "type": "array",
      "items": { "$ref": "#/definitions/uuid" }
    },
    "string": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "exceptionCode": {
      "type": "string",
      "enum": [
        "CONFLICT_EXCEPTION",
        "GENERAL_PROJECT_EXCEPTION",
        "ITEM_NOT_FOUND_EXCEPTION",
        "UNAUTHORIZED_EXCEPTION",
        "UNPRIVILEGED_EXCEPTION",
        "INVALID_INPUT_EXCEPTION",
        "ISSUE_STATE_EXCEPTION"
      ]
    },
    "defeatStrength": {
      "type": "string",
      "enum": [
        "TUPLE",
        "SIMPLE"
      ]
    },
    "issueState": {
      "type": "string",
      "enum": [
        "ADMISSION",
        "DISCUSSION",
        "VERIFICATION",
        "VOTING",
        "CANCELED_BY_ADMIN",
        "CANCELED_REVOKED_BEFORE_ACCEPTED",
        "CANCELED_ISSUE_NOT_ACCEPTED",
        "CANCELED_AFTER_REVOCATION_DURING_DISCUSSION",
        "CANCELED_AFTER_REVOCATION_DURING_VERIFICATION",
        "CANCELED_NO_INITIATIVE_ADMITTED",
        "FINISHED_TABULATING_RESULTS",
        "FINISHED_WITHOUT_WINNER",
        "FINISHED_WITH_WINNER"
      ]
    },
    "int": {
      "type": "integer"
    },
    "tieBreaking": {
      "type": "string",
      "enum": [
        "SIMPLE",
        "VARIANT1",
        "VARIANT2"
      ]
    },
    "exceptionType": {
      "type": "string",
      "enum": [
        "COMMAND_EVALUATION_EXCEPTION"
      ]
    }
  }
}
